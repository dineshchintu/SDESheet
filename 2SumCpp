class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> an; 
        unordered_map<int, int> mpp; 
        for(int i = 0;i<nums.size();i++) {
            if(mpp.find(target - nums[i]) != mpp.end()) {
                an.push_back(mpp[target-nums[i]]); 
                an.push_back(i); 
                return ans; 
            }
            mpp[nums[i]] = i; 
        }
        return an; 
    }
};
